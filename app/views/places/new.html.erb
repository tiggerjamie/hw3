<h1>New Place</h1>

<form action="/places" method="post">
  <p>
    <label for="name">Place Name</label>
    <input type="text" name="name" id="name">
  </p>
  <p>
  <input type="hidden" name="created_at" value="<%= Time.current %>">
  <input type="hidden" name="updated_at" value="<%= Time.current %>">
  </p>
  <button>Submit</button>
</form>
